class Cylinder{
    constructor(radius,height){
        this.radius=radius;
        this.height=height;

    }
    getVolume(){
        let vol=(22/7)*(this.radius*this.radius)*(this.height);
        console.log(vol.toFixed(4));
    }
}
class Sphere{
    constructor(radius,height){
        this.radius=radius;
        this.height=height;

    }
    getVolume(){
        let vol=(4/3)*(22/7)*(this.radius*this.radius*this.radius);
        console.log(vol.toFixed(4));
        
        
    }

}
class Cone{
    constructor(radius,height){
        this.radius=radius;
        this.height=height;

    }
    getVolume(){
        let vol=(22/7)*(this.radius*this.radius)*(this.height/3);
        console.log(vol.toFixed(4));
    }
}
