function replace(value) {
    let string1=""
    let string = "";
    let a = value.indexOf(" ");
    let len = value.length;
    for (let i = a; i <= len - 1; i++) {
        if (value[i] === " ") {
            string1=string1+value[i];

            continue;
        }
        else {
            string = string + value[i];
        }
    }
    let b = value.slice(0, a);
    let c = string;
    console.log(string +string1+ b);
}

