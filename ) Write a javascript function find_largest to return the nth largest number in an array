let arra1 = 0;
function nthLargest(arra, highest) {
  for (let i = 1; i <= highest - 1; i++) {
    let b = Math.max.apply(Math, arra);
    let c = arra.indexOf(b);
    let d = arra.splice(c, 1);
    arra1 = arra;
  }
  console.log(Math.max.apply(Math, arra1));
}

nthLargest([23, 4, 3, 32, 34, 4], 3);
